      "fontStyle": "italic"
    },
    "decorator": {
      "foreground": "#ff7a5f",
      "fontStyle": "underline"
    },
    "label": "#51b04f",
    "event": "#a9c99f",
    "boolean": "#f3c09a",
    "function.declaration": {
      "foreground": "#f498c0",
      "fontStyle": "bold"
    },
    "method.declaration": {
      "foreground": "#ffaacf",
      "fontStyle": "bold"
    },
    "variable.readonly": {
      "foreground": "#99bfcf",
      "fontStyle": "italic"
    },
    "property.readonly": {
      "foreground": "#57b0ff",
      "fontStyle": "italic"
    },
    "parameter.declaration": {
      "foreground": "#8fcfd0",
      "fontStyle": "underline"
    },
    "enumMember.declaration": {
      "foreground": "#deb07a",
      "fontStyle": "bold"
    },
    "class.defaultLibrary": {
      "foreground": "#3fc489",
      "fontStyle": "italic"
    },
    "type.defaultLibrary": {
      "foreground": "#12b4ff",
      "fontStyle": "italic"
    },
    "namespace.defaultLibrary": {
      "foreground": "#65c5a8",
      "fontStyle": "italic"
    },
    "keyword.async": {
      "foreground": "#d0b0ff",
      "fontStyle": "italic"
    },
    "function.async": {
      "foreground": "#f498c0",
      "fontStyle": "italic"
    }
  }
}
