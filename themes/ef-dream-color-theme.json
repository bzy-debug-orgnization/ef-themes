  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#caa89f"
      }
    },
    {
      "name": "Docstrings",
      "scope": [
        "comment.documentation",
        "comment.block.documentation",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#99bfcf"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#d0b0ff"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#8f8886"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type",
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#3fc489"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#f498c0"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#65c5a8"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.character",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#12b4ff"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#80aadf"
      }
    },
    {
      "name": "Strings: Escape and Regex",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#ffaacf"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#8f8886"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ff7a5f"
      }
    },
    {
      "name": "Markup: Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#ff6f6f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#80aadf",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup: Links",
      "scope": [
        "meta.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#80aadf",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Lists and Quotes",
      "scope": [
        "markup.list",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#b0a0cf"
      }
    },
    {
      "name": "Markup: Inline Code",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#f498c0"
      }
    },
    {
      "name": "Diff: Inserted",
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#a0d0f0"
      }
    },
    {
      "name": "Diff: Deleted",
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file"
      ],
      "settings": {
        "foreground": "#f0bfcf"
      }
    },
    {
      "name": "Diff: Changed",
      "scope": [
        "markup.changed",
        "meta.diff.range"
      ],
      "settings": {
        "foreground": "#dada90"
      }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#65c5a8",
    "class": "#3fc489",
    "struct": "#7fce5f",
    "enum": "#c0b24f",
    "interface": {
      "foreground": "#6fb3c0",
      "fontStyle": "italic"
    },
    "typeParameter": "#e3b0c0",
    "type": "#12b4ff",
