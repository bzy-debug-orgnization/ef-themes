    "function": "#f498c0",
    "method": "#ffaacf",
    "property": "#57b0ff",
    "parameter": "#8fcfd0",
    "variable": "#99bfcf",
    "enumMember": "#deb07a",
    "constant": "#a0a0cf",
    "macro": "#ff6f6f",
