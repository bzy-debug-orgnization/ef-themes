  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a05b5f"
      }
    },
    {
      "name": "Docstrings",
      "scope": [
        "comment.documentation",
        "comment.block.documentation",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#5f60bf"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#705ae3"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#786e74"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type",
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#007f6f"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#c035aa"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#0f7a9d"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.character",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#065fbf"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#5165e4"
      }
    },
    {
      "name": "Strings: Escape and Regex",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#ad45ba"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#786e74"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#d03033"
      }
    },
    {
      "name": "Markup: Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#c3303a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#5165e4",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup: Links",
      "scope": [
        "meta.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#5165e4",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Lists and Quotes",
      "scope": [
        "markup.list",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#3f7668"
      }
    },
    {
      "name": "Markup: Inline Code",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#c035aa"
      }
    },
    {
      "name": "Diff: Inserted",
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#005000"
      }
    },
    {
      "name": "Diff: Deleted",
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file"
      ],
      "settings": {
        "foreground": "#8f1313"
      }
    },
    {
      "name": "Diff: Changed",
      "scope": [
        "markup.changed",
        "meta.diff.range"
      ],
      "settings": {
        "foreground": "#553d00"
      }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#0f7a9d",
    "class": "#007f6f",
    "struct": "#4f7d0f",
    "enum": "#a45f22",
    "interface": {
      "foreground": "#1f6fbf",
      "fontStyle": "italic"
    },
    "typeParameter": "#804fb0",
    "type": "#065fbf",
    "typeAlias": "#5165e4",
    "function": "#c035aa",
    "method": "#ad45ba",
    "property": "#375cd6",
    "parameter": "#3f6faf",
    "variable": "#5f60bf",
    "enumMember": "#b65050",
    "constant": "#6060d0",
    "macro": "#c3303a",
    "keyword": "#705ae3",
    "modifier": "#c01f5f",
    "string": "#3f7668",
    "number": "#b8532f",
    "regexp": "#a24568",
    "operator": "#786e74",
    "comment": {
      "foreground": "#a05b5f",
      "fontStyle": "italic"
    },
    "decorator": {
      "foreground": "#d03033",
      "fontStyle": "underline"
    },
    "label": "#057800",
    "event": "#41754c",
    "boolean": "#4f45ff",
    "function.declaration": {
      "foreground": "#c035aa",
      "fontStyle": "bold"
    },
    "method.declaration": {
      "foreground": "#ad45ba",
      "fontStyle": "bold"
    },
    "variable.readonly": {
      "foreground": "#5f60bf",
      "fontStyle": "italic"
    },
    "property.readonly": {
      "foreground": "#375cd6",
      "fontStyle": "italic"
    },
    "parameter.declaration": {
      "foreground": "#3f6faf",
      "fontStyle": "underline"
    },
    "enumMember.declaration": {
      "foreground": "#b65050",
      "fontStyle": "bold"
    },
    "class.defaultLibrary": {
      "foreground": "#007f6f",
      "fontStyle": "italic"
    },
    "type.defaultLibrary": {
      "foreground": "#065fbf",
      "fontStyle": "italic"
    },
    "namespace.defaultLibrary": {
      "foreground": "#0f7a9d",
      "fontStyle": "italic"
    },
    "keyword.async": {
      "foreground": "#705ae3",
      "fontStyle": "italic"
    },
    "function.async": {
      "foreground": "#c035aa",
      "fontStyle": "italic"
    }
  }
}
