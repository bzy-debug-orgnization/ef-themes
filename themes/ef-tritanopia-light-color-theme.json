  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#776d6a"
      }
    },
    {
      "name": "Docstrings",
      "scope": [
        "comment.documentation",
        "comment.block.documentation",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#5f6a90"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#af40af"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#756275"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type",
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#008058"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#bf256a"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#007faa"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.character",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#065fff"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#4250ef"
      }
    },
    {
      "name": "Strings: Escape and Regex",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#aa357f"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#756275"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#dd0000"
      }
    },
    {
      "name": "Markup: Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#aa0010",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup: Links",
      "scope": [
        "meta.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Lists and Quotes",
      "scope": [
        "markup.list",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#194f90"
      }
    },
    {
      "name": "Markup: Inline Code",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#bf256a"
      }
    },
    {
      "name": "Diff: Inserted",
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#004840"
      }
    },
    {
      "name": "Diff: Deleted",
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file"
      ],
      "settings": {
        "foreground": "#8f1313"
      }
    },
    {
      "name": "Diff: Changed",
      "scope": [
        "markup.changed",
        "meta.diff.range"
      ],
      "settings": {
        "foreground": "#333399"
      }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#007faa",
    "class": "#008058",
    "struct": "#4a7d00",
    "enum": "#805d00",
    "interface": {
      "foreground": "#2070af",
      "fontStyle": "italic"
    },
    "typeParameter": "#7f4580",
    "type": "#065fff",
    "typeAlias": "#4250ef",
    "function": "#bf256a",
    "method": "#aa357f",
    "property": "#375cd8",
    "parameter": "#2f5faf",
    "variable": "#5f6a90",
    "enumMember": "#765040",
    "constant": "#6060d0",
    "macro": "#aa0010",
    "keyword": "#af40af",
    "modifier": "#c50f4f",
    "string": "#194f90",
    "number": "#965000",
    "regexp": "#92454f",
    "operator": "#756275",
    "comment": {
      "foreground": "#776d6a",
      "fontStyle": "italic"
    },
    "decorator": {
      "foreground": "#dd0000",
      "fontStyle": "underline"
    },
    "label": "#217a3c",
    "event": "#61756c",
    "boolean": "#bb0000",
    "function.declaration": {
      "foreground": "#bf256a",
      "fontStyle": "bold"
    },
    "method.declaration": {
      "foreground": "#aa357f",
      "fontStyle": "bold"
    },
    "variable.readonly": {
      "foreground": "#5f6a90",
      "fontStyle": "italic"
    },
    "property.readonly": {
      "foreground": "#375cd8",
      "fontStyle": "italic"
    },
    "parameter.declaration": {
      "foreground": "#2f5faf",
      "fontStyle": "underline"
    },
    "enumMember.declaration": {
      "foreground": "#765040",
      "fontStyle": "bold"
    },
    "class.defaultLibrary": {
      "foreground": "#008058",
      "fontStyle": "italic"
    },
    "type.defaultLibrary": {
      "foreground": "#065fff",
      "fontStyle": "italic"
    },
    "namespace.defaultLibrary": {
      "foreground": "#007faa",
      "fontStyle": "italic"
    },
    "keyword.async": {
      "foreground": "#af40af",
      "fontStyle": "italic"
    },
    "function.async": {
      "foreground": "#bf256a",
      "fontStyle": "italic"
    }
  }
}
