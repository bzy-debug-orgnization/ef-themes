  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7f6f1a"
      }
    },
    {
      "name": "Docstrings",
      "scope": [
        "comment.documentation",
        "comment.block.documentation",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#4f6f8f"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#6448ca"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#68708a"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type",
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#008250"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#b02f80"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#0f708a"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.character",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#265fbf"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#5f5fdf"
      }
    },
    {
      "name": "Strings: Escape and Regex",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#aa3e74"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#68708a"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#c74400"
      }
    },
    {
      "name": "Markup: Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#ba2d2f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#5f5fdf",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup: Links",
      "scope": [
        "meta.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#5f5fdf",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Lists and Quotes",
      "scope": [
        "markup.list",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#80431a"
      }
    },
    {
      "name": "Markup: Inline Code",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#b02f80"
      }
    },
    {
      "name": "Diff: Inserted",
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#005000"
      }
    },
    {
      "name": "Diff: Deleted",
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file"
      ],
      "settings": {
        "foreground": "#8f1313"
      }
    },
    {
      "name": "Diff: Changed",
      "scope": [
        "markup.changed",
        "meta.diff.range"
      ],
      "settings": {
        "foreground": "#553d00"
      }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#0f708a",
    "class": "#008250",
    "struct": "#5a7400",
    "enum": "#a26310",
    "interface": {
      "foreground": "#3f60af",
      "fontStyle": "italic"
    },
    "typeParameter": "#905ea0",
    "type": "#265fbf",
    "typeAlias": "#5f5fdf",
    "function": "#b02f80",
    "method": "#aa3e74",
    "property": "#375cc6",
    "parameter": "#4060a0",
    "variable": "#4f6f8f",
    "enumMember": "#946830",
    "constant": "#4a659f",
    "macro": "#ba2d2f",
    "keyword": "#6448ca",
    "modifier": "#c02945",
    "string": "#80431a",
    "number": "#ba5205",
    "regexp": "#b05350",
    "operator": "#68708a",
    "comment": {
      "foreground": "#7f6f1a",
      "fontStyle": "italic"
    },
    "decorator": {
      "foreground": "#c74400",
      "fontStyle": "underline"
    },
    "label": "#007a0a",
    "event": "#61756c",
    "boolean": "#a07f00",
    "function.declaration": {
      "foreground": "#b02f80",
      "fontStyle": "bold"
    },
    "method.declaration": {
      "foreground": "#aa3e74",
      "fontStyle": "bold"
    },
    "variable.readonly": {
      "foreground": "#4f6f8f",
      "fontStyle": "italic"
    },
    "property.readonly": {
      "foreground": "#375cc6",
      "fontStyle": "italic"
    },
    "parameter.declaration": {
      "foreground": "#4060a0",
      "fontStyle": "underline"
    },
    "enumMember.declaration": {
      "foreground": "#946830",
      "fontStyle": "bold"
    },
    "class.defaultLibrary": {
      "foreground": "#008250",
      "fontStyle": "italic"
    },
    "type.defaultLibrary": {
      "foreground": "#265fbf",
      "fontStyle": "italic"
    },
    "namespace.defaultLibrary": {
      "foreground": "#0f708a",
      "fontStyle": "italic"
    },
    "keyword.async": {
      "foreground": "#6448ca",
      "fontStyle": "italic"
    },
    "function.async": {
      "foreground": "#b02f80",
      "fontStyle": "italic"
    }
  }
}
