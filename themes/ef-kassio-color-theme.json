  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a65d6a"
      }
    },
    {
      "name": "Docstrings",
      "scope": [
        "comment.documentation",
        "comment.block.documentation",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#506fa0"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#7022bf"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#776f79"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type",
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#008358"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#9f248a"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#1077ab"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.character",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#065fff"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#4250ef"
      }
    },
    {
      "name": "Strings: Escape and Regex",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#a01f64"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#776f79"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#e00033"
      }
    },
    {
      "name": "Markup: Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#b00234",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup: Links",
      "scope": [
        "meta.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Lists and Quotes",
      "scope": [
        "markup.list",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#af4988"
      }
    },
    {
      "name": "Markup: Inline Code",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#9f248a"
      }
    },
    {
      "name": "Diff: Inserted",
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#005000"
      }
    },
    {
      "name": "Diff: Deleted",
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file"
      ],
      "settings": {
        "foreground": "#8f1313"
      }
    },
    {
      "name": "Diff: Changed",
      "scope": [
        "markup.changed",
        "meta.diff.range"
      ],
      "settings": {
        "foreground": "#553d00"
      }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#1077ab",
    "class": "#008358",
    "struct": "#4a7d00",
    "enum": "#9a6012",
    "interface": {
      "foreground": "#2f5f9f",
      "fontStyle": "italic"
    },
    "typeParameter": "#954f90",
    "type": "#065fff",
    "typeAlias": "#4250ef",
    "function": "#9f248a",
    "method": "#a01f64",
    "property": "#3c3bbe",
    "parameter": "#3f6faf",
    "variable": "#506fa0",
    "enumMember": "#a04646",
    "constant": "#6060d0",
    "macro": "#b00234",
    "keyword": "#7022bf",
    "modifier": "#d5305f",
    "string": "#af4988",
    "number": "#b6530f",
    "regexp": "#af3f5f",
    "operator": "#776f79",
    "comment": {
      "foreground": "#a65d6a",
      "fontStyle": "italic"
    },
    "decorator": {
      "foreground": "#e00033",
      "fontStyle": "underline"
    },
    "label": "#217a3c",
    "event": "#61756c",
    "boolean": "#d06f30",
    "function.declaration": {
      "foreground": "#9f248a",
      "fontStyle": "bold"
    },
    "method.declaration": {
      "foreground": "#a01f64",
      "fontStyle": "bold"
    },
    "variable.readonly": {
      "foreground": "#506fa0",
      "fontStyle": "italic"
    },
    "property.readonly": {
      "foreground": "#3c3bbe",
      "fontStyle": "italic"
    },
    "parameter.declaration": {
      "foreground": "#3f6faf",
      "fontStyle": "underline"
    },
    "enumMember.declaration": {
      "foreground": "#a04646",
      "fontStyle": "bold"
    },
    "class.defaultLibrary": {
      "foreground": "#008358",
      "fontStyle": "italic"
    },
    "type.defaultLibrary": {
      "foreground": "#065fff",
      "fontStyle": "italic"
    },
    "namespace.defaultLibrary": {
      "foreground": "#1077ab",
      "fontStyle": "italic"
    },
    "keyword.async": {
      "foreground": "#7022bf",
      "fontStyle": "italic"
    },
    "function.async": {
      "foreground": "#9f248a",
      "fontStyle": "italic"
    }
  }
}
