  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a65f6a"
      }
    },
    {
      "name": "Docstrings",
      "scope": [
        "comment.documentation",
        "comment.block.documentation",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#506fa0"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage", "storage.modifier"],
      "settings": {
        "foreground": "#6052cf"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#68759f"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type",
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#008858"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#cf25aa"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable", "variable.other"],
      "settings": {
        "foreground": "#1f77bb"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.character",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#065fff"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#4250ef"
      }
    },
    {
      "name": "Strings: Escape and Regex",
      "scope": ["constant.character.escape", "string.regexp"],
      "settings": {
        "foreground": "#ba35af"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#68759f"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#e00033"
      }
    },
    {
      "name": "Markup: Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#d3303a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup: Links",
      "scope": ["meta.link", "markup.underline.link"],
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Lists and Quotes",
      "scope": ["markup.list", "markup.quote"],
      "settings": {
        "foreground": "#397a70"
      }
    },
    {
      "name": "Markup: Inline Code",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#cf25aa"
      }
    },
    {
      "name": "Diff: Inserted",
      "scope": ["markup.inserted", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#005000"
      }
    },
    {
      "name": "Diff: Deleted",
      "scope": ["markup.deleted", "meta.diff.header.from-file"],
      "settings": {
        "foreground": "#8f1313"
      }
    },
    {
      "name": "Diff: Changed",
      "scope": ["markup.changed", "meta.diff.range"],
      "settings": {
        "foreground": "#553d00"
      }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#1f77bb",
    "class": "#008858",
    "struct": "#008858",
    "enum": "#008858",
    "interface": "#008858",
    "typeParameter": "#008858",
    "type": "#008858",
    "function": "#cf25aa",
    "method": "#cf25aa",
    "property": "#065fff",
    "parameter": "#1f77bb",
    "variable": "#1f77bb",
    "enumMember": "#065fff",
    "constant": "#065fff",
    "macro": "#d3303a",
    "keyword": "#6052cf",
    "modifier": "#6052cf",
    "string": "#4250ef",
    "number": "#065fff",
    "regexp": "#ba35af",
    "operator": "#68759f",
    "comment": {
      "foreground": "#a65f6a",
      "italic": true
    },
    "decorator": "#ba35af"
  }
}
