  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9a5f6a"
      }
    },
    {
      "name": "Docstrings",
      "scope": [
        "comment.documentation",
        "comment.block.documentation",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#5f60bf"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#8e44f3"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#786e74"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type",
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#007f68"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#cb1aaa"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#0f7b8f"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.character",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#065fff"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#5250ef"
      }
    },
    {
      "name": "Strings: Escape and Regex",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#ba35af"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#786e74"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#e00033"
      }
    },
    {
      "name": "Markup: Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#d3303a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#5250ef",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup: Links",
      "scope": [
        "meta.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#5250ef",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Lists and Quotes",
      "scope": [
        "markup.list",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#af4988"
      }
    },
    {
      "name": "Markup: Inline Code",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#cb1aaa"
      }
    },
    {
      "name": "Diff: Inserted",
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#005000"
      }
    },
    {
      "name": "Diff: Deleted",
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file"
      ],
      "settings": {
        "foreground": "#8f1313"
      }
    },
    {
      "name": "Diff: Changed",
      "scope": [
        "markup.changed",
        "meta.diff.range"
      ],
      "settings": {
        "foreground": "#553d00"
      }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#0f7b8f",
    "class": "#007f68",
    "struct": "#4a7d00",
    "enum": "#a45f22",
    "interface": {
      "foreground": "#1f6fbf",
      "fontStyle": "italic"
    },
    "typeParameter": "#a45392",
    "type": "#065fff",
    "typeAlias": "#5250ef",
    "function": "#cb1aaa",
    "method": "#ba35af",
    "property": "#375ce6",
    "parameter": "#3f6faf",
    "variable": "#5f60bf",
    "enumMember": "#b65050",
    "constant": "#6060d0",
    "macro": "#d3303a",
    "keyword": "#8e44f3",
    "modifier": "#d50f7f",
    "string": "#af4988",
    "number": "#b6532f",
    "regexp": "#c24552",
    "operator": "#786e74",
    "comment": {
      "foreground": "#9a5f6a",
      "fontStyle": "italic"
    },
    "decorator": {
      "foreground": "#e00033",
      "fontStyle": "underline"
    },
    "label": "#217a3c",
    "event": "#61756c",
    "boolean": "#cf0090",
    "function.declaration": {
      "foreground": "#cb1aaa",
      "fontStyle": "bold"
    },
    "method.declaration": {
      "foreground": "#ba35af",
      "fontStyle": "bold"
    },
    "variable.readonly": {
      "foreground": "#5f60bf",
      "fontStyle": "italic"
    },
    "property.readonly": {
      "foreground": "#375ce6",
      "fontStyle": "italic"
    },
    "parameter.declaration": {
      "foreground": "#3f6faf",
      "fontStyle": "underline"
    },
    "enumMember.declaration": {
      "foreground": "#b65050",
      "fontStyle": "bold"
    },
    "class.defaultLibrary": {
      "foreground": "#007f68",
      "fontStyle": "italic"
    },
    "type.defaultLibrary": {
      "foreground": "#065fff",
      "fontStyle": "italic"
    },
    "namespace.defaultLibrary": {
      "foreground": "#0f7b8f",
      "fontStyle": "italic"
    },
    "keyword.async": {
      "foreground": "#8e44f3",
      "fontStyle": "italic"
    },
    "function.async": {
      "foreground": "#cb1aaa",
      "fontStyle": "italic"
    }
  }
}
