  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7f4f4a"
      }
    },
    {
      "name": "Docstrings",
      "scope": [
        "comment.documentation",
        "comment.block.documentation",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#3f627f"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#5f2fba"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#676470"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type",
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#007047"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#9a3a6a"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#006f70"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.character",
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#003faf"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#444fcf"
      }
    },
    {
      "name": "Strings: Escape and Regex",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#80308f"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#676470"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#d00000"
      }
    },
    {
      "name": "Markup: Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#c3303a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#444fcf",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup: Links",
      "scope": [
        "meta.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#444fcf",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Lists and Quotes",
      "scope": [
        "markup.list",
        "markup.quote"
      ],
      "settings": {
        "foreground": "#3f4f99"
      }
    },
    {
      "name": "Markup: Inline Code",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#9a3a6a"
      }
    },
    {
      "name": "Diff: Inserted",
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#005000"
      }
    },
    {
      "name": "Diff: Deleted",
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file"
      ],
      "settings": {
        "foreground": "#8f1313"
      }
    },
    {
      "name": "Diff: Changed",
      "scope": [
        "markup.changed",
        "meta.diff.range"
      ],
      "settings": {
        "foreground": "#553d00"
      }
    }
  ],
  "semanticTokenColors": {
    "namespace": "#006f70",
    "class": "#007047",
    "struct": "#3a6f00",
    "enum": "#805a1f",
    "interface": {
      "foreground": "#1f66af",
      "fontStyle": "italic"
    },
    "typeParameter": "#6a4a9f",
    "type": "#003faf",
    "typeAlias": "#444fcf",
    "function": "#9a3a6a",
    "method": "#80308f",
    "property": "#375cc6",
    "parameter": "#305675",
    "variable": "#3f627f",
    "enumMember": "#78542f",
    "constant": "#3a5099",
    "macro": "#c3303a",
    "keyword": "#5f2fba",
    "modifier": "#b02440",
    "string": "#3f4f99",
    "number": "#8b4400",
    "regexp": "#8b4052",
    "operator": "#676470",
    "comment": {
      "foreground": "#7f4f4a",
      "fontStyle": "italic"
    },
    "decorator": {
      "foreground": "#d00000",
      "fontStyle": "underline"
    },
    "label": "#007010",
    "event": "#4a763f",
    "boolean": "#036f99",
    "function.declaration": {
      "foreground": "#9a3a6a",
      "fontStyle": "bold"
    },
    "method.declaration": {
      "foreground": "#80308f",
      "fontStyle": "bold"
    },
    "variable.readonly": {
      "foreground": "#3f627f",
      "fontStyle": "italic"
    },
    "property.readonly": {
      "foreground": "#375cc6",
      "fontStyle": "italic"
    },
    "parameter.declaration": {
      "foreground": "#305675",
      "fontStyle": "underline"
    },
    "enumMember.declaration": {
      "foreground": "#78542f",
      "fontStyle": "bold"
    },
    "class.defaultLibrary": {
      "foreground": "#007047",
      "fontStyle": "italic"
    },
    "type.defaultLibrary": {
      "foreground": "#003faf",
      "fontStyle": "italic"
    },
    "namespace.defaultLibrary": {
      "foreground": "#006f70",
      "fontStyle": "italic"
    },
    "keyword.async": {
      "foreground": "#5f2fba",
      "fontStyle": "italic"
    },
    "function.async": {
      "foreground": "#9a3a6a",
      "fontStyle": "italic"
    }
  }
}
